{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=react-fabric2 && changeset publish",
    "site": "turbo run build --filter=@yxf/astro"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "concurrently": "^9.1.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "name": "react-fabric2",
  "packageManager": "pnpm@9.4.0"
}
